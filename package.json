{
  "name": "karlhaworth-com_eleventy",
  "version": "1.0.0",
  "description": "[![Netlify Status](https://api.netlify.com/api/v1/badges/1cc341c5-8fbf-45f7-8b7d-8ad204a1a5a8/deploy-status)](https://app.netlify.com/sites/karlhaworth/deploys)",
  "main": ".eleventy.js",
  "dependencies": {
    "lightbox2": "^2.11.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "popper.js": "^1.16.1",
    "puppeteer": "^5.2.1"
  },
  "scripts": {
    "build": "npm-run-all build:css build:html build:screenshots",
    "build:css": "node-sass --output ./_site/assets/css/ ./_sass/main.scss",
    "build:html": "eleventy",
    "build:screenshots": "node screenshot.js",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "node-sass --watch --output ./_site/assets/css/ ./_sass/main.scss",
    "watch:html": "eleventy --serve",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/karlhaworth/karlhaworth.com_11ty.git"
  },
