{
  "name": "karlhaworth-com_eleventy",
  "version": "1.0.0",
  "description": "npm install -g @11ty/eleventy npm install eleventy-plugin-sass --save npm install @quasibit/eleventy-plugin-sitemap --save",
  "main": ".eleventy.js",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "popper.js": "^1.16.1"
  },
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "node-sass --output ./_site/assets/css/ ./_sass/main.scss",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "node-sass --watch --output ./_site/assets/css/ ./_sass/main.scss",
    "watch:html": "eleventy --serve",
    "start": "npm run watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
