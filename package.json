{
  "name": "karlhaworth-com_eleventy",
  "version": "1.0.0",
  "description": "npm install -g @11ty/eleventy npm install eleventy-plugin-sass --save npm install @quasibit/eleventy-plugin-sitemap --save",
  "main": ".eleventy.js",
  "dependencies": {
    "@quasibit/eleventy-plugin-sitemap": "^2.0.0",
    "eleventy-plugin-sass": "^1.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@quasibit/eleventy-plugin-sitemap": "^2.0.0",
    "autoprefixer": "^9.8.6",
    "bootstrap": "^4.5.2",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "jquery": "^3.5.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "popper.js": "^1.16.1"
  },
  "scripts": {
    "scss": "node-sass --output-style compressed -o src/site/_includes/css src/scss",
    "watch:css": "onchange 'src/scss' -- npm run scss",
    "serve": "eleventy --serve",
    "start": "run-p serve watch:css",
    "clean": "rm -rf _site/*",
    "build": "npm run clean && eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
